{
  "description": "Secret detection patterns extracted from gitleaks (https://github.com/gitleaks/gitleaks)",
  "source": "https://github.com/gitleaks/gitleaks/blob/master/config/gitleaks.toml",
  "version": "8.21.2",
  "rules": [
    {
      "id": "aws-access-token",
      "description": "AWS Access Token",
      "regex": "\\b((?:A3T[A-Z0-9]|AKIA|ASIA|ABIA|ACCA)[A-Z2-7]{16})\\b"
    },
    {
      "id": "github-pat",
      "description": "GitHub Personal Access Token",
      "regex": "ghp_[0-9a-zA-Z]{36}"
    },
    {
      "id": "github-fine-grained-pat",
      "description": "GitHub Fine-Grained Personal Access Token",
      "regex": "github_pat_\\w{82}"
    },
    {
      "id": "github-oauth",
      "description": "GitHub OAuth Access Token",
      "regex": "gho_[0-9a-zA-Z]{36}"
    },
    {
      "id": "github-app-token",
      "description": "GitHub App Token",
      "regex": "ghu_[0-9a-zA-Z]{36}"
    },
    {
      "id": "github-refresh-token",
      "description": "GitHub Refresh Token",
      "regex": "ghr_[0-9a-zA-Z]{36}"
    },
    {
      "id": "gitlab-pat",
      "description": "GitLab Personal Access Token",
      "regex": "glpat-[0-9a-zA-Z\\-_]{20}"
    },
    {
      "id": "openai-api-key",
      "description": "OpenAI API Key",
      "regex": "\\b(sk-(?:proj|svcacct|admin)-[A-Za-z0-9_-]{74,140}T3BlbkFJ[A-Za-z0-9_-]{20,80}|sk-[a-zA-Z0-9]{20}T3BlbkFJ[a-zA-Z0-9]{20})\\b"
    },
    {
      "id": "anthropic-api-key",
      "description": "Anthropic API Key",
      "regex": "\\b(sk-ant-api03-[a-zA-Z0-9_\\-]{93}AA)"
    },
    {
      "id": "anthropic-admin-api-key",
      "description": "Anthropic Admin API Key",
      "regex": "\\b(sk-ant-admin01-[a-zA-Z0-9_\\-]{93}AA)"
    },
    {
      "id": "stripe-access-token",
      "description": "Stripe Access Token",
      "regex": "\\b((?:sk|rk)_(?:test|live|prod)_[a-zA-Z0-9]{10,99})\\b"
    },
    {
      "id": "stripe-publishable-key",
      "description": "Stripe Publishable Key",
      "regex": "\\b(pk_(?:test|live)_[a-zA-Z0-9]{10,99})\\b"
    },
    {
      "id": "slack-bot-token",
      "description": "Slack Bot Token",
      "regex": "xoxb-[0-9]{10,13}-[0-9]{10,13}[a-zA-Z0-9-]*"
    },
    {
      "id": "slack-user-token",
      "description": "Slack User Token",
      "regex": "xoxp-[0-9]{10,13}-[0-9]{10,13}-[0-9]{10,13}-[a-zA-Z0-9]{32}"
    },
    {
      "id": "slack-webhook-url",
      "description": "Slack Webhook URL",
      "regex": "https?://hooks\\.slack\\.com/(?:services|workflows|triggers)/[A-Za-z0-9+/]{43,56}"
    },
    {
      "id": "discord-api-token",
      "description": "Discord API Token",
      "regex": "(?i)discord[a-z0-9_ .\\-,]{0,25}['\"][0-9a-zA-Z]{64}['\"]"
    },
    {
      "id": "discord-webhook",
      "description": "Discord Webhook URL",
      "regex": "https?://(?:ptb\\.|canary\\.)?discord(?:app)?\\.com/api/webhooks/[0-9]{17,20}/[A-Za-z0-9_-]{60,68}"
    },
    {
      "id": "twilio-api-key",
      "description": "Twilio API Key",
      "regex": "SK[0-9a-fA-F]{32}"
    },
    {
      "id": "sendgrid-api-key",
      "description": "SendGrid API Key",
      "regex": "SG\\.[a-zA-Z0-9_-]{22}\\.[a-zA-Z0-9_-]{43}"
    },
    {
      "id": "mailgun-api-key",
      "description": "Mailgun API Key",
      "regex": "key-[0-9a-zA-Z]{32}"
    },
    {
      "id": "mailchimp-api-key",
      "description": "Mailchimp API Key",
      "regex": "[0-9a-f]{32}-us[0-9]{1,2}"
    },
    {
      "id": "gcp-api-key",
      "description": "GCP API Key",
      "regex": "AIza[0-9A-Za-z_-]{35}"
    },
    {
      "id": "firebase-api-key",
      "description": "Firebase API Key",
      "regex": "AIza[0-9A-Za-z_-]{35}"
    },
    {
      "id": "heroku-api-key",
      "description": "Heroku API Key",
      "regex": "(?i)heroku[a-z0-9_ .\\-,]{0,25}['\"][0-9A-F]{8}-[0-9A-F]{4}-[0-9A-F]{4}-[0-9A-F]{4}-[0-9A-F]{12}['\"]"
    },
    {
      "id": "npm-access-token",
      "description": "npm Access Token",
      "regex": "npm_[A-Za-z0-9]{36}"
    },
    {
      "id": "pypi-api-token",
      "description": "PyPI API Token",
      "regex": "pypi-[A-Za-z0-9_-]{50,}"
    },
    {
      "id": "private-key",
      "description": "Private Key",
      "regex": "-----BEGIN[A-Z0-9 _-]*PRIVATE KEY[A-Z0-9 _-]*-----"
    },
    {
      "id": "jwt",
      "description": "JSON Web Token",
      "regex": "\\b(ey[a-zA-Z0-9]{17,}\\.ey[a-zA-Z0-9/_-]{17,}\\.[a-zA-Z0-9/_-]{10,})"
    },
    {
      "id": "bearer-token",
      "description": "Bearer Token in Authorization Header",
      "regex": "(?i)bearer\\s+[a-zA-Z0-9_\\-./+]{20,}"
    },
    {
      "id": "basic-auth",
      "description": "Basic Auth Credentials",
      "regex": "(?i)basic\\s+[a-zA-Z0-9+/]{20,}={0,2}"
    },
    {
      "id": "postgres-connection-string",
      "description": "PostgreSQL Connection String",
      "regex": "postgres(?:ql)?://[^:]+:[^@]+@[^/]+"
    },
    {
      "id": "mysql-connection-string",
      "description": "MySQL Connection String",
      "regex": "mysql://[^:]+:[^@]+@[^/]+"
    },
    {
      "id": "mongodb-connection-string",
      "description": "MongoDB Connection String",
      "regex": "mongodb(?:\\+srv)?://[^:]+:[^@]+@[^/]+"
    },
    {
      "id": "redis-connection-string",
      "description": "Redis Connection String",
      "regex": "redis://[^:]+:[^@]+@[^/]+"
    },
    {
      "id": "vercel-access-token",
      "description": "Vercel Access Token",
      "regex": "(?i)vercel[a-z0-9_ .\\-,]{0,25}['\"][a-zA-Z0-9]{24}['\"]"
    },
    {
      "id": "netlify-access-token",
      "description": "Netlify Access Token",
      "regex": "(?i)netlify[a-z0-9_ .\\-,]{0,25}['\"][a-zA-Z0-9_-]{40,46}['\"]"
    },
    {
      "id": "supabase-api-key",
      "description": "Supabase API Key",
      "regex": "sbp_[a-f0-9]{40}"
    },
    {
      "id": "planetscale-password",
      "description": "PlanetScale Password",
      "regex": "pscale_pw_[a-zA-Z0-9_-]{43}"
    },
    {
      "id": "neon-api-key",
      "description": "Neon API Key",
      "regex": "neon_[a-zA-Z0-9_-]{32,}"
    },
    {
      "id": "linear-api-key",
      "description": "Linear API Key",
      "regex": "lin_api_[a-zA-Z0-9]{40}"
    },
    {
      "id": "doppler-api-token",
      "description": "Doppler API Token",
      "regex": "dp\\.(?:ct|st|sa|scim|audit)\\.[a-zA-Z0-9]{40,44}"
    },
    {
      "id": "sentry-auth-token",
      "description": "Sentry Auth Token",
      "regex": "sntrys_[a-zA-Z0-9]{64}"
    },
    {
      "id": "algolia-api-key",
      "description": "Algolia API Key",
      "regex": "(?i)algolia[a-z0-9_ .\\-,]{0,25}['\"][a-f0-9]{32}['\"]"
    },
    {
      "id": "shopify-access-token",
      "description": "Shopify Access Token",
      "regex": "shpat_[a-fA-F0-9]{32}"
    },
    {
      "id": "shopify-shared-secret",
      "description": "Shopify Shared Secret",
      "regex": "shpss_[a-fA-F0-9]{32}"
    },
    {
      "id": "square-access-token",
      "description": "Square Access Token",
      "regex": "sq0atp-[0-9A-Za-z_-]{22}"
    },
    {
      "id": "square-oauth-secret",
      "description": "Square OAuth Secret",
      "regex": "sq0csp-[0-9A-Za-z_-]{43}"
    },
    {
      "id": "age-secret-key",
      "description": "Age Secret Key",
      "regex": "AGE-SECRET-KEY-1[QPZRY9X8GF2TVDW0S3JN54KHCE6MUA7L]{58}"
    },
    {
      "id": "1password-service-account",
      "description": "1Password Service Account Token",
      "regex": "ops_[a-zA-Z0-9_-]{80,}"
    },
    {
      "id": "hashicorp-vault-token",
      "description": "HashiCorp Vault Token",
      "regex": "hvs\\.[a-zA-Z0-9_-]{24,}"
    }
  ]
}
